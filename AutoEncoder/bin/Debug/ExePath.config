<?xml version="1.0" encoding="utf-8" ?>
<Config Type="Exe">
  <DgIndex>
    <Path>Library\DGIndex.exe</Path>
    <InputExt>.ts</InputExt>
    <OutputExt>.d2v</OutputExt>
    <Option>-IF=[{Input}] -OF=[{Output}] -ia 5 -fo 0 -yr 2 -om 1 -minimize -EXIT</Option>
  </DgIndex>
  <ToWave>
    <Path>Library\ToWave.exe</Path>
    <InputExt>.aac</InputExt>
    <OutputExt>.wav</OutputExt>
    <Option> {Input}</Option>
  </ToWave>
  <Ts2Aac>
    <Path>Library\ts2aac.exe</Path>
    <InputExt>.ts</InputExt>
    <OutputExt>.aac</OutputExt>
    <Option> -i "{Input}" -o "{Output}" -B</Option>
  </Ts2Aac>
</Config>