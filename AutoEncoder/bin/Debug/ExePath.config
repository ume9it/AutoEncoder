<?xml version="1.0" encoding="utf-8" ?>
<Config Type="Exe">
  <DgIndex>
    <Path>App\DGIndex\DGIndex.exe</Path>
    <InputExt>.ts</InputExt>
    <OutputExt>.d2v</OutputExt>
    <Option>-IF=[{Input}] -OF=[{Output}] -ia 5 -fo 0 -yr 2 -om 1 -minimize -EXIT</Option>
  </DgIndex>
  <ToWave>
    <Path>App\ToWave\ToWave.exe</Path>
    <InputExt>.aac</InputExt>
    <OutputExt>.wav</OutputExt>
    <Option> {Input}</Option>
  </ToWave>
  <Ts2Aac>
    <Path>App\ts2aac\ts2aac.exe</Path>
    <InputExt>.ts</InputExt>
    <OutputExt>.aac</OutputExt>
    <Option> -i "{Input}" -o "{Output}" -B</Option>
  </Ts2Aac>
  <ChapterExe>
    <Path>App\chapter_exe\chapter_exe.exe</Path>
    <InputExt>avs</InputExt>
    <OutputExt>txt</OutputExt>
    <Option> -v "{Input}" -o "{Output}" -m 100 -s 15</Option>
  </ChapterExe>
</Config>